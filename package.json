{
  "name": "test-random",
  "version": "1.0.0",
  "description": "Adapter for shipments",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "tsc",
    "build": "tsc --project ./",
    "start": "node build/index.js",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.{json,ts}\"",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.ts",
    "typecheck": "tsc --project ./ --noEmit"
  },
  "author": "dromix",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run typecheck"
    }
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/express": "^4.17.12",
    "env-cmd": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
